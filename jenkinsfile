pipeline{
environment {
registry = "https://hub.docker.com//jaya91"
registryCredential = 'dockerhub_cred'
    agent any
    stages {
        stage('pwd') {
            steps {
                sh 'ls ; pwd'
            }
        }
    }
    stages {
        stage('build') {
            steps {
                sh 'docker build -t jaya91/web:1.$BUILD_NUMBER .'
                sh 'docker images'
            }
        }
stage('Deploy our image') {
steps{
script {
docker.withRegistry( '', registryCredential ) {
dockerImage.push()
        stage('run'){
            steps{
                docker.image('jaya91/web:1.$BUILD_NUMBER').withRun('-p 2233:80') 
                docker.withRegistry('https://hub.docker.com/', 'dockerhub_cred') 
            }
        }
        }
    
}
}
